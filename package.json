		"chalk": "^5.3.0",
		"cross-env": "^7.0.3",
		"esbuild": "^0.25.0",
		"eslint": "^8.57.0",
		"eslint-plugin-eslint-rules": "file:eslint-rules",
		"grpc-tools": "^1.13.0",
